{
    "name": "wds-learn-css",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "apps/courses/wds-learn-css/src",
    "tags": [],
    "targets": {
        "build": {
            "executor": "@nx/webpack:webpack",
            "outputs": ["{options.outputPath}"],
            "defaultConfiguration": "production",
            "options": {
                "outputPath": "dist/apps/courses/wds-learn-css",
                "compiler": "swc",
                "main": "apps/courses/wds-learn-css/src/main.ts",
                "tsConfig": "apps/courses/wds-learn-css/tsconfig.app.json",
                "webpackConfig": "apps/courses/wds-learn-css/webpack.config.js",
                "assets": [
                    "apps/courses/wds-learn-css/src/favicon.ico",
                    "apps/courses/wds-learn-css/src/assets"
                ],
                "index": "apps/courses/wds-learn-css/src/index.html",
                "baseHref": "/",
                "styles": ["apps/courses/wds-learn-css/src/styles.css"],
                "scripts": []
            },
            "configurations": {
                "production": {
                    "optimization": true,
                    "outputHashing": "all",
                    "sourceMap": false,
                    "namedChunks": false,
                    "extractLicenses": true,
                    "vendorChunk": false,
                    "fileReplacements": [
                        {
                            "replace": "apps/courses/wds-learn-css/src/environments/environment.ts",
                            "with": "apps/courses/wds-learn-css/src/environments/environment.prod.ts"
                        }
                    ]
                }
            }
        },
        "serve": {
            "executor": "@nx/webpack:dev-server",
            "options": {
                "buildTarget": "wds-learn-css:build",
            },
            "configurations": {
                "production": {
                    "buildTarget": "wds-learn-css:build:production"
                }
            }
        },
        "lint": {
            "executor": "@nx/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/courses/wds-learn-css/**/*.ts"]
            }
        }
    }
}
